            <Rectangle2D height="982.0" width="1512.0" />
         </viewport>
      </ImageView>
      <ChoiceBox fx:id="choicebox" layoutX="409.0" layoutY="177.0" prefHeight="35.0" prefWidth="294.0" />
      <Button fx:id="D_button" layoutX="741.0" layoutY="177.0" mnemonicParsing="false" onMouseClicked="#deletSlot" prefHeight="35.0" prefWidth="131.0" text="Delete Reservation " />
      <Label layoutX="416.0" layoutY="114.0" text="Your Reservations :">
         <font>
            <Font name="Bell MT" size="28.0" />
         </font>
      </Label>
      <Button fx:id="R_button" layoutX="910.0" layoutY="177.0" mnemonicParsing="false" onMouseClicked="#replaceReservation" prefHeight="35.0" prefWidth="131.0" text="Replace Reservation " />
